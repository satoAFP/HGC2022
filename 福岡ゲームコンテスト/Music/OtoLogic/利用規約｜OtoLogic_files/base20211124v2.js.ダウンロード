/* slideout準備 */
      var slideout = new Slideout({
        'panel': document.getElementById('slideout-idsitei-main'),// コンテンツ部を指定。こことhtmlソースを変えるだけでOK
        'menu': document.getElementById('slideout-idsitei-menu'),// ドロワー部を指定。こことhtmlソースを変えるだけでOK
        'padding': 280,
        'tolerance': 70// スライドで開ける時の「開きそうで開かない遊び部分」の長さ指定
      });






/* メニューを開けるボタンの設定 */
document.querySelector('.js-slideout-toggle').addEventListener('click', function() {
  slideout.open();
});

function close(eve) {
  eve.preventDefault();
  slideout.close();
}


$('.sample').on("click", function(){
	slideout.close();
});



/*ドロワーの状態に合わせCSSを動かしたりするための処理 */
slideout

  .on('beforeopen', function() {//ドロワー展開（開始）時に処理
    this.panel.classList.add('panel-open');//公式改造　コンテンツ部の暗転とスクロール禁止用
    $("#drw-menu").fadeOut(200);//自作  fixedバグ対策　MENUボタンを消す
    $("#drw-menu2").fadeIn(200);//自作  fixedバグ対策　×ボタンを出す
  })

  .on('translatestart', function() {//自作　スライドによるドロワー展開（操作開始）時に処理　上記beforeopenではスワイプしてる最中に発火しない為（処理がダブるケースは大丈夫なのだろうか？エラー等は出ていないが）
    this.panel.classList.add('panel-open');//同上
    $("#drw-menu").fadeOut(200);//同上
    $("#drw-menu2").fadeIn(200);//同上
  })

  .on('open', function() {//ドロワー展開（完了）時に処理
    this.panel.addEventListener('click', close);//公式改造　暗転部クリックで閉じる用？
  })

  .on('beforeclose', function() {//ドロワー格納（開始）時に処理
    this.panel.classList.remove('panel-open');//公式改造　もとに戻す時のクラス除去？
    this.panel.removeEventListener('click', close);//公式改造　もとに戻す時のクラス除去？
    $("#drw-menu2").fadeOut(50);//自作  fixedバグ対策　出した×ボタンを消す
  })

  .on('close', function() {//ドロワー格納（完了）時に処理
    $("#drw-menu").fadeIn(100);//自作  fixedバグ対策　消したボタンをもとに戻す
  })

;
//上記の「公式改造」とは、標準では組み込まれてなかったが、後になってFAQ内で「暗転やスクロール禁止等を実装したい場合はこうやってみ？」と公式が示したソースのこと






/*leftのクローンを作成 */

$(function() {
    $('.left').clone().appendTo('#slideout-idsitei-menu');
 
});
//クローンってことはメニューがｐｃ用モバイル用の2つに分かれて、開閉状況等はリンクしないんだよねって問題点はあるけど、そもそもそんな同時に頻繁に切り替えて使うケースなんて無いから問題ないでしょ（ヤケクソ）
//ってかとにかく、leftを完全に移植する形にすると、既存のHTML・CSSの改造がくそ煩雑になるので、それはやりたくない（）





/* バグ防止の為、サイズ変えたら閉じる
$(window).resize(function(){
slideout.close();
});
 */

/* PCサイズで見てる時、タッチデバイスでは、スワイプ動作でメニューが開けてしまうため（もう表示されてるので無意味）、それを無効化 */
/* また、モバイルサイズからｐｃサイズになった時、メニューを閉じる（もともとはリサイズがあったら閉じる処理だったが（上記）、アンドロイドのアドレスバーでバグるからこちらへ） */

$(function() {//画面読み込み時の解像度を取得
    var w = $(window).width();
    var x = 820;
    if (w <= x) {

slideout.enableTouch();//タッチ（スワイプ）によるメニュー操作有効

    } else {

slideout.disableTouch();//タッチ（スワイプ）によるメニュー操作無効
slideout.close();

    }
});

$(window).resize(function(){//リサイズがあった場合の最新値を取得
    var w = $(window).width();
    var x = 820;
//ここから下は同上
    if (w <= x) {

slideout.enableTouch();

    } else {

slideout.disableTouch();
slideout.close();

    }
});




/* アコーディオン (leftをクローンする後に置かないと、クローン部分が動作しないよ)*/
$(function () { 
	$('nav .toggle_menu_1').click(function() { 
		$(this).toggleClass("active");
		$(this).next('ul').slideToggle(); 
	}); 
	$('nav .tm3_trigger').click(function(e) { 
		$(this).closest('li').toggleClass("active");
		$(this).next('ul').slideToggle();  
		e.stopPropagation();
	}); 
	$('nav .tm4_trigger').click(function(e) { 
		$(this).closest('li').toggleClass("active");
		$(this).next('ul').slideToggle();  
		e.stopPropagation();
	}); 
});

$(document).ready(function(){
	$('.accordion_head').click(function() {
		$(this).toggleClass("active");
	$(this).next().slideToggle();
	}).next().hide();
});







